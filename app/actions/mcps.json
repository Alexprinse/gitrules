{
    "GitHub": {
        "type": "http",
        "url": "https://api.githubcopilot.com/mcp",
        "headers": {
            "Authorization": "Bearer ${GITHUB_TOKEN}"
        }
    },
    "Firecrawl": {
        "type": "sse",
        "url": "https://mcp.firecrawl.dev/${FIRECRAWL_API_KEY}/sse"
    },
    "Playwright": {
        "type": "stdio",
        "command": "npx",
        "args": [
            "@playwright/mcp@latest"
        ]
    },
    "Supabase": {
        "command": "npx",
        "args": [
            "-y",
            "@supabase/mcp-server-supabase@latest",
            "--access-token",
            "${SUPABASE_ACCESS_TOKEN}"
        ]
    }
}